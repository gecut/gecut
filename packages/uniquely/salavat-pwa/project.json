{
  "name": "uniquely-salavat-pwa",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/uniquely/salavat-pwa/src",
  "targets": {
    "build": {
      "executor": "@nx-plus/vue:browser",
      "options": {
        "dest": "dist/packages/uniquely/salavat-pwa",
        "index": "packages/uniquely/salavat-pwa/public/index.html",
        "main": "packages/uniquely/salavat-pwa/src/main.ts",
        "tsConfig": "packages/uniquely/salavat-pwa/tsconfig.app.json"
      },
      "configurations": {
        "production": {
          "mode": "production",
          "filenameHashing": true,
          "productionSourceMap": true,
          "css": {
            "extract": true,
            "sourceMap": false
          }
        }
      }
    },
    "serve": {
      "executor": "@nx-plus/vue:dev-server",
      "options": {
        "browserTarget": "uniquely-salavat-pwa:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "uniquely-salavat-pwa:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/uniquely/salavat-pwa/**/*.{ts,tsx,vue}"]
      }
    }
  },
  "tags": []
}
